{
  "name": "@fepvenancio/stela-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/fepvenancio/stela-sdk-ts.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.2.0",
  "description": "TypeScript SDK for the Stela P2P lending protocol on StarkNet",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "src/abi"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "starknet": "^6.0.0"
  },
  "devDependencies": {
    "starknet": "^6.23.1",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "keywords": ["starknet", "stela", "p2p", "lending", "defi", "sdk"],
  "license": "MIT"
}
